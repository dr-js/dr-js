{
  "private": true,
  "name": "dr-js",
  "version": "0.22.0-dev.0",
  "description": "A collection of strange functions",
  "author": "dr-js",
  "license": "MIT",
  "repository": "github:dr-js/dr-js",
  "bin": "bin/index.js",
  "browser": "library/Dr.browser.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script quiet pack",
    "script-pack-test": "babel-node ./script verbose pack test",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "script-generate-spec": "cross-env BABEL_ENV=use-source babel-node ./script/generateSpec ./tempFileDelete.config.json",
    "script-delete-temp-build-file": "cross-env BABEL_ENV=use-source babel-node ./source-bin -c ./tempFileDelete.config.json",
    "// build =======================": "",
    "build-bin": "babel ./source-bin --out-dir ./output-gitignore/bin --copy-files",
    "build-bin-dev": "cross-env BABEL_ENV=dev babel ./source-bin --out-dir ./output-gitignore/bin --copy-files --watch",
    "build-library-babel": "babel ./source --out-dir ./output-gitignore/library --ignore source/browser/",
    "build-library-babel-dev": "cross-env BABEL_ENV=dev npm run build-library-babel -- --watch",
    "build-library-webpack": "babel-node ./script/webpack production profile",
    "build-library-webpack-dev": "babel-node ./script/webpack development watch",
    "build-module": "cross-env BABEL_ENV=module babel ./source --out-dir ./output-gitignore/module",
    "// test ========================": "",
    "test-dev": "cross-env BABEL_ENV=dev dr-dev --TR @babel/register -T source/common/data/CacheMap.test.js",
    "test-source": "dr-dev --TR @babel/register -T source/env/ source/common/ source/node/ --TFS .test.js",
    "test-output-library": "dr-dev -T output-gitignore/library/env/ output-gitignore/library/common/ output-gitignore/library/node/ --TFS .test.js",
    "test-output-module": "dr-dev --TR @babel/register -T output-gitignore/module/env/ output-gitignore/module/common/ output-gitignore/module/node/ --TFS .test.js",
    "// test-browser ================": "",
    "test-browser": "babel-node ./script/testBrowser production",
    "test-browser-dev": "babel-node ./script/testBrowser development",
    "// =============================": "",
    "analyze-stat": "npx webpack-bundle-analyzer .temp-gitignore/profile-stat.json",
    "lint": "eslint ./source/ ./source-bin/ ./script/ ./example/",
    "test": "npm run script-pack-test",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=10.15",
    "npm": ">=6"
  },
  "devDependencies": {
    "dr-dev": "0.0.6-dev.5",
    "dr-dev-web-puppeteer": "0.0.6-dev.5"
  },
  "sideEffects": false
}
