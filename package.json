{
  "name": "dr-js",
  "version": "0.0.4",
  "description": "A collection of strange functions",
  "main": "library/Dr.js",
  "scripts": {
    "prof-example-server": "cross-env NODE_ENV=production node --prof example/example-server.js",
    "example": "cross-env NODE_ENV=production webpack --config config/example.conf.js",
    "example-dev": "cross-env NODE_ENV=development webpack --watch --config config/example.conf.js",
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js",
    "library-dev": "cross-env NODE_ENV=development webpack --watch --config config/library.conf.js",
    "webpack-stats": "cross-env NODE_ENV=production webpack -p --config config/library.conf.js --json > stats.json",
    "webpack-profile": "cross-env NODE_ENV=production webpack -p --config config/library.conf.js --profile",
    "test-mocha": "mocha --compilers js:babel-register --recursive test",
    "test": "npm run build && npm run test-mocha",
    "build": "npm run library",
    "prepublish": "npm run build",
    "start": "npm run example-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr-js/dr-js.git"
  },
  "keywords": [
    "Dr",
    "JavaScript"
  ],
  "author": "dr-js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dr-js/dr-js/issues"
  },
  "homepage": "https://github.com/dr-js/dr-js#readme",
  "devDependencies": {
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-env": "*",
    "babel-require": "*",
    "babel-runtime": "*",
    "cross-env": "*",
    "eslint": "*",
    "eslint-config-standard": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "mocha": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*"
  }
}
